<p id="notice"><%= notice %></p>
<div class="page-header">
<h1>Acessos</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Horário de Entrada</th>
      <th>Horário de Saída</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @acessos.each do |acesso| %>
      <tr>
        <td><%= acesso.horarioentrada %></td>
        <td><%= acesso.horariosaida %></td>
        <td><%= link_to 'Ver', acesso %></td>
        <td><%= link_to 'Editar', edit_acesso_path(acesso) %></td>
        <td><%= link_to 'Destruir', acesso, method: :delete, data: { confirm: 'Você tem certeza?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Criar Acesso', new_acesso_path %>
